{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "globalEnv": ["NODE_ENV", "DATABASE_URL"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "migrate": {
      "outputs": []
    },
    "generate": {
      "env": ["DATABASE_URL"],
      "dependsOn": ["^generate"],
      "cache": false
    }
  }
}
